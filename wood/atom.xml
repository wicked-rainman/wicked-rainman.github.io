---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Wicked Rainman's woodwork projects</title>
	<link href="http://wicked-rainman.github.io/wood/atom.xml" rel="self"/>
	<link href="http://wicked-rainman.github.io/wood"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://wicked-rainman.github.io/wood</id>
	<author>
		<name>Wicked Rainman</name>
	</author>

	{% for post in site.wood %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://wicked-rainman.github.io{{ wood.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://wicked-rainman.github.io{{ post.id }}</id>
			<content type="html">{{ wood.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
